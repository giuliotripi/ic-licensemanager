{
  "canisters": {
    "hello": {
      "build": [
        "cargo build --target wasm32-unknown-unknown --package hello --release"
      ],
      "wasm": "target/wasm32-unknown-unknown/release/hello.wasm",
      "type": "custom",
      "candid": "src/hello/hello.did",
      "package": "hello"
    },
    "hello_assets": {
      "dependencies": [
        "hello"
      ],
      "frontend": {
        "entrypoint": "src/hello_assets/src/index.html"
      },
      "source": [
        "src/hello_assets/assets",
        "dist/hello_assets/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "dfx": "0.9.3",
  "networks": {
    "local": {
      "bind": "0.0.0.0:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}